{
  "name": "meland",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen --debug --output-dir src/generated/entities/",
    "build": "graph build",
    "prepare:mumbai": "node ./genconfig.js config/mumbai.json && mustache config/mumbai.json subgraph.template.yaml > subgraph.yaml && npm run codegen",
    "prepare:local": "node ./genconfig.js config/local.json && mustache config/local.json subgraph.template.yaml > subgraph.yaml && npm run codegen",
    "prepare:matic": "node ./genconfig.js config/matic.json && mustache config/matic.json subgraph.template.yaml > subgraph.yaml && npm run codegen",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ alberthuang24/sword-subgraph",
    "create:local": "graph create --node http://localhost:8020/ alberthuang24/sword-subgraph",
    "deploy:local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 alberthuang24/sword-subgraph",
    "deploy:matic": "graph deploy --ipfs https://api-ipfs.melandworld.com --node https://jsonrpc-graph-node-matic.melandworld.com alberthuang24/sword-subgraph",
    "deploy:mumbai": "graph deploy --ipfs https://api-ipfs.melandworld.com --node https://jsonrpc-graph-node-mumbai.melandworld.com alberthuang24/sword-subgraph",
    "create:mumbai": "graph create --node https://jsonrpc-graph-node-mumbai.melandworld.com alberthuang24/sword-subgraph",
    "create:matic": "graph create --node https://jsonrpc-graph-node-matic.melandworld.com alberthuang24/sword-subgraph"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.24.0",
    "@graphprotocol/graph-ts": "^0.24.0",
    "as-crypto": "^0.0.4",
    "mustache": "^4.0.1"
  },
  "devDependencies": {
    "assemblyscript": "0.19.10"
  },
  "resolutions": {
    "@graphprotocol/graph-ts": "https://github.com/Meland-Inc/graph-ts.git#master"
  }
}
